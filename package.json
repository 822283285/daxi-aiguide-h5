{
  "name": "daxi-aiguide-h5",
  "version": "1.0.0",
  "description": "大希智能导游 H5 应用",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "health-check": "node scripts/health-check/generate-health-check.js",
    "ci": "node scripts/ci/minimal-ci.js"
  },
  "keywords": [
    "daxi",
    "aiguide",
    "h5",
    "tourism",
    "guide"
  ],
  "author": "大希团队",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/daxi-aiguide/daxi-aiguide-h5.git"
  },
  "devDependencies": {
    "@vitejs/plugin-legacy": "^5.3.0",
    "eslint": "^9.0.0",
    "globals": "^15.0.0",
    "jest": "^29.7.0",
    "terser": "^5.27.0",
    "vite": "^5.1.0"
  },
  "dependencies": {
    "crypto-js": "^4.2.0",
    "zepto": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "pnpm": {
    "allowedDeprecatedVersions": {
      "zepto": "*"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "eslint"
      ]
    }
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "iOS >= 10",
    "Android >= 5"
  ]
}
